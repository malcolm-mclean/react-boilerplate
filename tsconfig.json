{
	"compilerOptions": {
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "./dist/",
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"target": "ES6"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}

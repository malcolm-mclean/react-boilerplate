{
	"name": "react-boilerplate",
	"version": "0.0.0",
	"main": "src/index.ts",
	"repository": "https://github.com/malcolm-mclean/react-boilerplate.git",
	"author": "Malcolm McLean",
	"license": "MIT",
	"private": true,
	"scripts": {
		"analyze": "yarn run build:stats && webpack-bundle-analyzer stats.json dist/ --mode static",
		"analyze:ci": "yarn run build:stats && webpack-bundle-analyzer stats.json dist/ --mode json",
		"build": "yarn run clean && yarn run lint && webpack --config webpack.prod.config.js",
		"build:stats": "yarn run clean && webpack --config webpack.prod.config.js --json > stats.json",
		"clean": "rimraf dist/",
		"lint": "eslint --ext .ts,.tsx src/",
		"start": "webpack serve --config webpack.dev.config.js",
		"test": "yarn run lint && jest",
		"test:coverage": "jest --coverage",
		"test:ci:coverage": "jest --coverage --config jest.ci.config.js"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-react": "^7.12.13",
		"@babel/preset-typescript": "^7.13.0",
		"@malcolm-mclean/eslint-config": "malcolm-mclean/eslint-config#4.0.2",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.5",
		"@types/jest": "^26.0.21",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"@types/react-router-dom": "^5.1.7",
		"autoprefixer": "^10.2.5",
		"babel-loader": "^8.2.2",
		"babel-plugin-jsx-remove-data-test-id": "^3.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.0",
		"danger": "^10.7.1",
		"danger-plugin-code-coverage": "^1.1.8",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.3.1",
		"jest": "^26.6.3",
		"mini-css-extract-plugin": "^1.3.9",
		"postcss": "^8.2.13",
		"postcss-loader": "^5.2.0",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"sass": "^1.35.2",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.1.1",
		"ts-jest": "^26.5.4",
		"typescript": "^4.2.3",
		"webpack": "^5.28.0",
		"webpack-bundle-analyzer": "^4.4.0",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.7.3"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0"
	}
}
